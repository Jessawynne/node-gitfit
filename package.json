{
  "name": "node-gitfit",
  "version": "1.0.0",
  "description": "Nashville Software School project to create a simple fitness app with Heroku, SQLite and Node.js",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "serve": "nodemon server.js",
    "heroku": "git push origin master && git push heroku master && heroku open && heroku logs --tail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jessawynne/node-gitfit.git"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "no-extra-semi": 0
    }
  },
  "author": "Jessawynne Parker <jessawynne@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jessawynne/node-gitfit/issues"
  },
  "homepage": "https://github.com/Jessawynne/node-gitfit#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "highcharts": "^4.2.3",
    "jade": "^1.11.0",
    "sqlite3": "^3.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.9.1"
  }
}
